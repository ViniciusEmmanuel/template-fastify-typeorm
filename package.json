{
  "name": "template-fastify-typeorm",
  "version": "1.0.0",
  "description": "Template para Api com Fastify e TypeOrm com configurações pré definidas",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --transpile-only --ignore node_modules src/index.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.1.1",
    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^4.0.3",
    "fastify-multer": "^2.0.1",
    "fastify-static": "^3.2.0",
    "fastify-swagger": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "pino": "^6.6.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.27",
    "@types/pino": "^6.3.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "pino-pretty": "^4.2.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
